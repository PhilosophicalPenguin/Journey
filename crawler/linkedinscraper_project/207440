                    var images1 = Array();
                    function checkLoadMediaAd() {

                        if (mediaClicks == numClicksB4Ad) {
                            mediaClicks = 0;
                            showAd();
                            return true;
                        }
                        else {
                            mediaClicks++;
                            hideAd();
                            return false;
                        }
                    }
                    function showAd() {
                        jQuery('#mediaImageItems').css("display", "none");
                        jQuery('#mediaImageAd').css("display","block");
                        AnalyticsCustomEvent('MediaAd', 'story_300x250_slide');
                        jQuery('#adframe_imageInterstitial').attr("src","/controlpage.aspx?control=googleadmanagercontrolajax&adname=story_300x250_slide&sitepagename=ViewStory&adsection=crime-cour&width=300&height=250");
                        if (jQuery('#LargeImagesLink') != undefined) {
                            jQuery('#LargeImagesLink').css("display","none");
                        }
                    }
                    function hideAd() {
                        jQuery('#mediaImageItems').css("display", "block");
                        jQuery('#mediaImageAd').css("display", "none");
                        jQuery('#adframe_imageInterstitial').attr("src", "");
                        if (jQuery('#LargeImagesLink') != undefined) {
                           jQuery('#LargeImagesLink').css("display", "block");
                        }
                    }
                </script>
